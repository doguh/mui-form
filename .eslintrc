{
  "parser": "babel-eslint",
  "extends": ["standard", "standard-react"],
  "env": {
    "es6": true
  },
  "plugins": ["react"],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "semi": 0,
    "quotes": 0,
    "jsx-quotes": 0,
    "space-before-function-paren": 0,
    "react/jsx-boolean-value": 0
  }
}
